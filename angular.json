{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "koala-angular-tools": {
      "projectType": "library",
      "root": "projects/koala-angular-tools",
      "sourceRoot": "projects/koala-angular-tools/src",
      "prefix": "k",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/koala-angular-tools/tsconfig.lib.json",
            "project": "projects/koala-angular-tools/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/koala-angular-tools/src/test.ts",
            "tsConfig": "projects/koala-angular-tools/tsconfig.spec.json",
            "karmaConfig": "projects/koala-angular-tools/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/koala-angular-tools/tsconfig.lib.json",
              "projects/koala-angular-tools/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "tester": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/tester",
      "sourceRoot": "projects/tester/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/tester",
            "index": "projects/tester/src/index.html",
            "main": "projects/tester/src/main.ts",
            "polyfills": "projects/tester/src/polyfills.ts",
            "tsConfig": "projects/tester/tsconfig.app.json",
            "aot": false,
            "assets": [
              "projects/tester/src/favicon.ico",
              "projects/tester/src/assets"
            ],
            "styles": [
              "projects/tester/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/tester/src/environments/environment.ts",
                  "with": "projects/tester/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "tester:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "tester:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "tester:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/tester/src/test.ts",
            "polyfills": "projects/tester/src/polyfills.ts",
            "tsConfig": "projects/tester/tsconfig.spec.json",
            "karmaConfig": "projects/tester/karma.conf.js",
            "assets": [
              "projects/tester/src/favicon.ico",
              "projects/tester/src/assets"
            ],
            "styles": [
              "projects/tester/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/tester/tsconfig.app.json",
              "projects/tester/tsconfig.spec.json",
              "projects/tester/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/tester/e2e/protractor.conf.js",
            "devServerTarget": "tester:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "tester:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "koala-angular-tools"
}